<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.FirstActivity.SecondFragment.SecondFragment">


    <TextView
        android:id="@+id/textView25"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/tahoma"
        android:gravity="center"
        android:text="@string/label_yas"
        android:textColor="@color/black"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/tahoma"
        android:gravity="center"
        android:text="@string/label_yas_desc"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/textView25"
        app:layout_constraintStart_toStartOf="@+id/textView25"
        app:layout_constraintTop_toBottomOf="@+id/textView25" />


    <TextView
        android:id="@+id/textView39"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/tahoma"
        android:gravity="center"
        android:text="@string/lbl_day"
        android:textColor="@color/black"
        android:textSize="25sp"
        android:layout_marginTop="30dp"
        app:layout_constraintEnd_toEndOf="@+id/number_picker_day"
        app:layout_constraintStart_toStartOf="@+id/textView26"
        app:layout_constraintTop_toBottomOf="@+id/textView26" />

    <TextView
        android:id="@+id/textView439"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/tahoma"
        android:gravity="center"
        android:text="@string/lbl_month"
        android:textColor="@color/black"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="@+id/number_picker_month"
        app:layout_constraintStart_toStartOf="@+id/number_picker_month"
        app:layout_constraintTop_toBottomOf="@+id/textView26" />
    <TextView
        android:id="@+id/textView459"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/tahoma"
        android:gravity="center"
        android:text="@string/lbl_year"
        android:textColor="@color/black"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="@+id/number_picker_year"
        app:layout_constraintStart_toStartOf="@+id/number_picker_year"
        app:layout_constraintTop_toBottomOf="@+id/textView26" />

    <com.shawnlin.numberpicker.NumberPicker
        android:id="@+id/number_picker_day"
        android:layout_width="70dp"
        android:layout_height="0dp"
        android:layout_centerInParent="true"
        app:layout_constraintBottom_toBottomOf="@+id/number_picker_month"
        app:layout_constraintEnd_toStartOf="@+id/number_picker_month"
        app:layout_constraintHorizontal_bias="0.862"
        app:layout_constraintStart_toStartOf="@+id/textView25"
        app:layout_constraintTop_toBottomOf="@+id/textView39"
        app:layout_constraintVertical_bias="0.0"
        app:np_dividerColor="@color/login_color"
        app:np_dividerDistance="50dp"
        app:np_dividerThickness="4dp"
        app:np_formatter="@string/number_picker_formatter"
        app:np_height="120dp"
        app:np_max="31"
        app:np_min="1"
        app:np_selectedTextColor="@color/login_color"
        app:np_selectedTextSize="@dimen/selected_text_size"
        app:np_textColor="@color/black"
        app:np_textSize="@dimen/text_size"
        app:np_typeface="serif"
        app:np_value="10"
        app:np_wheelItemCount="5"
        app:np_width="40dp" />

    <com.shawnlin.numberpicker.NumberPicker
        android:id="@+id/number_picker_month"
        android:layout_width="70dp"
        android:layout_height="0dp"
        android:layout_centerInParent="true"
        app:layout_constraintBottom_toBottomOf="@+id/number_picker_year"
        app:layout_constraintEnd_toStartOf="@+id/number_picker_year"
        app:layout_constraintTop_toBottomOf="@+id/textView439"
        app:layout_constraintVertical_bias="0.0"
        app:np_dividerColor="@color/login_color"
        app:np_dividerDistance="50dp"
        app:np_dividerThickness="4dp"
        app:np_formatter="@string/number_picker_formatter"
        app:np_height="120dp"
        app:np_max="12"
        app:np_min="1"
        app:np_selectedTextColor="@color/login_color"
        app:np_selectedTextSize="@dimen/selected_text_size"
        app:np_textColor="@color/black"
        app:np_textSize="@dimen/text_size"
        app:np_typeface="serif"
        app:np_value="5"
        app:np_wheelItemCount="5"
        app:np_width="40dp" />

    <com.shawnlin.numberpicker.NumberPicker
        android:id="@+id/number_picker_year"
        android:layout_width="100dp"
        android:layout_height="0dp"
        android:layout_centerInParent="true"
        android:layout_marginEnd="30dp"
        app:layout_constraintBottom_toTopOf="@+id/appCompatButton"
        app:layout_constraintEnd_toEndOf="@+id/textView25"
        app:layout_constraintTop_toBottomOf="@+id/textView459"
        app:layout_constraintVertical_bias="0.0"
        app:np_dividerColor="@color/login_color"
        app:np_dividerDistance="50dp"
        app:np_dividerThickness="4dp"
        app:np_formatter="@string/number_picker_formatter"
        app:np_height="120dp"
        app:np_max="2022"
        app:np_min="1930"
        app:np_selectedTextColor="@color/login_color"
        app:np_selectedTextSize="@dimen/selected_text_size"
        app:np_textColor="@color/black"
        app:np_textSize="@dimen/text_size"
        app:np_typeface="serif"
        app:np_value="2000"
        app:np_wheelItemCount="5"
        app:np_width="40dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/appCompatButton"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="15dp"
        android:background="@drawable/buttonfirst_background"
        android:fontFamily="@font/tahoma"
        android:text="@string/label_continue"
        android:textColor="@color/white"
        android:textSize="21sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/appCompatButton2" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/appCompatButton2"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="15dp"
        android:background="@drawable/buttonfirstback_background"
        android:fontFamily="@font/tahoma"
        android:text="@string/label_back"
        android:textColor="@color/white"
        android:textSize="21sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/appCompatButton"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>